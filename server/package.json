  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "jest": "18.1.0"
  },
  "devDependencies": {
    "babel-jest": "^19.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-node6": "^0.4.0"
  },
  "scripts": {
